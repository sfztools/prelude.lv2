@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<@LV2PLUGIN_URI@>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-preludelv2.html> ;
        modgui:stylesheet <modgui/stylesheet-preludelv2.css> ;
        modgui:screenshot <modgui/screenshot-preludelv2.png> ;
        modgui:thumbnail <modgui/thumbnail-preludelv2.png> ;
        modgui:brand "SFZTools" ;
        modgui:label "prelude" ;
        modgui:model "boxy" ;
        modgui:panel "2-knobs" ;
        modgui:color "wood2" ;
        modgui:knob "silver" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "flue" ;
            lv2:name "Flue ranks" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "reed" ;
            lv2:name "Reed ranks" ;
        ] ;
    ] .
